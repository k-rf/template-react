{
  "name": "template-react",
  "version": "1.1.0",
  "description": "The template of React.",
  "author": "Kawano Kenichi <kenichi.k.820@gmail.com>",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "typecheck": "tsc",
    "format": "prettier . --write",
    "lint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "check:all": "npm-run-all -s typecheck format lint"
  },
  "dependencies": {
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@remix-run/serve": "^2.11.2",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.14.2",
    "@eslint/js": "8.57.0",
    "@remix-run/dev": "^2.11.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "8.x",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.2.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sonarjs": "^2.0.2",
    "eslint-plugin-unused-imports": "^4.1.3",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
