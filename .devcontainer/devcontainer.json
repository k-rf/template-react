{
  "name": "template-react",
  "remoteUser": "template-react",
  "dockerComposeFile": "./compose.dev.yaml",
  "service": "app",
  "postCreateCommand": "op run -- pnpm install",
  "workspaceFolder": "/template-react",
  "overrideCommand": true,
  "shutdownAction": "none",
  "features": {
    "ghcr.io/itsmechlark/features/1password": "latest"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "albymor.increment-selection",
        "bodil.file-browser",
        "christian-kohler.path-intellisense",
        "DavidAnson.vscode-markdownlint",
        "dbaeumer.vscode-eslint",
        "eamodio.gitlens",
        "esbenp.prettier-vscode",
        "foxundermoon.shell-format",
        "GitHub.copilot-chat",
        "GitHub.copilot",
        "github.vscode-github-actions",
        "GitHub.vscode-pull-request-github",
        "Gruntfuggly.todo-tree",
        "hbenl.vscode-test-explorer",
        "ipatalas.vscode-postfix-ts",
        "irongeek.vscode-env",
        "kahole.magit",
        "mhutchie.git-graph",
        "mikestead.dotenv",
        "ms-azuretools.vscode-docker",
        "MS-vsliveshare.vsliveshare",
        "mutantdino.resourcemonitor",
        "MylesMurphy.prettify-ts",
        "Perkovec.emoji",
        "redhat.vscode-yaml",
        "shd101wyy.markdown-preview-enhanced",
        "streetsidesoftware.code-spell-checker",
        "stringham.copy-with-imports",
        "WallabyJs.console-ninja",
        "wmaurer.change-case",
        "yoavbls.pretty-ts-errors",
        "Yukai.map-replace-js",
        "yzhang.markdown-all-in-one"
      ],
      "settings": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "eslint.workingDirectories": [{ "mode": "auto" }],
        "eslint.experimental.useFlatConfig": true,
        "eslint.rules.customizations": [
          { "rule": "@cspell/spellchecker", "severity": "off" }
        ]
      }
    }
  }
}
